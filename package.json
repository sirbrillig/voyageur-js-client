{
  "name": "voyageur-js-client",
  "version": "1.0.0",
  "private": true,
  "description": "An app for calculating driving distances between multiple locations",
  "main": "build/bundle.js",
  "scripts": {
    "build": "NODE_PATH=$NODE_PATH:./app browserify app/boot.js --verbose --transform babelify -o build/bundle.js",
    "prebuild": "mkdirp build",
    "postbuild": "echo Build complete",
    "build:dist": "NODE_PATH=$NODE_PATH:./app NODE_ENV=production browserify app/boot.js --verbose --transform babelify --transform uglifyify | uglifyjs --compress --mangle -o build/bundle.js",
    "prebuild:dist": "mkdirp dist",
    "clean:dist": "rm -rf dist",
    "copy:dist": "cp -a build index.html 200.html assets app.css CNAME dist/",
    "dist": "npm-run-all clean:dist build:dist copy:dist",
    "webserver": "ws --port 3000 --spa 200.html",
    "watch": "onchange app -- npm run build",
    "start": "npm-run-all build --parallel webserver watch",
    "deploy": "surge --project dist",
    "predeploy": "npm run dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Payton Swick <payton@foolord.com>",
  "license": "MIT",
  "dependencies": {
    "auth0-lock": "^10.2.2",
    "auth0-lock-passwordless": "^2.2.3",
    "classnames": "^2.2.3",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "lodash.find": "^4.0.1",
    "lodash.findwhere": "^3.1.0",
    "lodash.flow": "^3.2.1",
    "lodash.get": "^4.0.0",
    "lodash.noop": "^3.0.0",
    "moment": "^2.11.2",
    "react": "^0.14.5",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^0.14.5",
    "react-google-maps": "^4.7.2",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0",
    "redux": "^3.0.5",
    "redux-debug": "^1.0.3",
    "redux-localstorage": "^0.4.0",
    "redux-thunk": "^1.0.3",
    "superagent": "^1.6.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "local-web-server": "^1.1.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "surge": "^0.18.0",
    "uglify-js": "^2.6.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.6.1"
  }
}
