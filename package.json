{
  "name": "voyageur-js-client",
  "version": "1.0.0",
  "private": true,
  "description": "An app for calculating driving distances between multiple locations",
  "main": "build/bundle.js",
  "scripts": {
    "build": "webpack --env development",
    "prebuild": "mkdirp build",
    "postbuild": "echo Build complete",
    "stats": "webpack --env production --profile --json > stats.json",
    "build:dist": "webpack -p",
    "prebuild:dist": "mkdirp dist",
    "clean:dist": "rimraf dist",
    "clean": "npm run clean:dist",
    "copy:dist": "cp -a build index.html 200.html assets fonts app.css dist/",
    "setname:dist": "cp CNAME dist/CNAME",
    "setname:stage": "cp CNAME-stage dist/CNAME",
    "dist": "npm-run-all clean:dist build:dist copy:dist setname:dist",
    "dist:stage": "npm-run-all clean:dist build:dist copy:dist setname:stage",
    "webserver": "ws --port 3000 --spa 200.html",
    "watch": "onchange app -- npm run build",
    "start": "npm-run-all build --parallel webserver watch",
    "deploy": "surge --project dist",
    "predeploy": "npm run dist",
    "stage": "npm run dist:stage && surge --project dist",
    "test": "NODE_PATH=$NODE_PATH:./app mocha --compilers js:babel-register"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    "env": {
      "production": {
        "presets": [
          "babili"
        ]
      }
    }
  },
  "author": "Payton Swick <payton@foolord.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.3",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "lodash.find": "^4.0.1",
    "lodash.findwhere": "^3.1.0",
    "lodash.flow": "^3.2.1",
    "lodash.get": "^4.0.0",
    "lodash.isequal": "^4.5.0",
    "lodash.noop": "^3.0.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-animate-on-change": "^0.2.1",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^15.4.1",
    "react-google-maps": "^4.7.2",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0",
    "redux": "^3.0.5",
    "redux-debug": "^1.0.3",
    "redux-localstorage": "^0.4.0",
    "redux-thunk": "^1.0.3",
    "superagent": "^1.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-babili": "0.0.9",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-wpcalypso": "^0.6.0",
    "eslint-plugin-wpcalypso": "^3.0.2",
    "local-web-server": "^1.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "surge": "^0.18.0",
    "webpack": "^2.2.0"
  }
}
